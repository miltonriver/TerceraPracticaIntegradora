<main>
    <h1>Bienvenido Usuario!!</h1>
    <form action="/api/sessions/loginpassport" method="POST">
        <label for="username" class="field_register_form">Nombre de usuario</label>
        <input type="text" name="username" placeholder="Ingrese su nombre de usuario" class="input_register_form">
        <label for="password" class="field_register_form">Contraseña</label>
        <div class="container">
            <input type="password" name="password" placeholder="Ingrese su contraseña" class="input_register_form" id="pass">
            <i class='icon_show bx bx-show'></i>
        </div><br>
        <button class="delete_button">
            Ingresar
        </button>
    </form>
    <br>
    <a href="/api/sessions/github" class="delete_button">Ingresar con GitHub</a>
</main>

<script src="/socket.io/socket.io.js"></script>
<script src="/js/index.js"></script>