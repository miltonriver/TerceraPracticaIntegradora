<main>
    <h1>Página de registro</h1>
    <form action="/api/sessions/registerpassport" method="POST">
        <label for="first_name" class="field_register_form">Nombre</label>
        <input type="text" name="first_name" placeholder="Ingrese su nombre" class="input_register_form">
        <label for="last_name" class="field_register_form">Apellido</label>
        <input type="text" name="last_name" placeholder="Ingrese su apellido" class="input_register_form">
        <label for="username" class="field_register_form">Usuario</label>
        <input type="text" name="username" placeholder="Ingrese su nombre de usuario" class="input_register_form">
        <label for="email" class="field_register_form">Email</label>
        <input type="text" name="email" placeholder="Ingrese su email" class="input_register_form">
        <label for="password" class="field_register_form">Contraseña</label>
        <input type="text" name="password" placeholder="Ingrese su contraseña" class="input_register_form">
        <label for="phone_number" class="field_register_form">Teléfono</label>
        <input type="number" name="phone_number" placeholder="Ingrese su número telefónico" class="input_register_form"><br><br>
        <button class="delete_button">
            Registrar
        </button>
    </form>
</main>